<div class="info-form-container">
{% include "@atoms/text/paragraph/paragraph.twig" with {
  "text": titleContact,
  "extra_classes": ['info-title']
} %}
  <div class="info-text">
    <input type='text' class=" txtbox txtName {{ nameClass }}" placeholder='{{ Name }}' />
    <input type='email' class='txtbox txtEmail {{ emailClass }}' placeholder='{{ Email }}' />
  </div>
  <div class="info-message">
    <textarea class="txtarea txtMsg {{ msgClass }}" placeholder='{{ Message }}'></textarea>
  </div>
  {% include "@atoms/button/button.twig" with {
    "button_title": "Submit",
    "variant": "info-btn"
  } %}
</div>
