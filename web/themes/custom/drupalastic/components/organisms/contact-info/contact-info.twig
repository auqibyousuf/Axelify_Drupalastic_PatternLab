<div class="contact-info-container">
  {% if heading %}
    {{ heading }}
  {% else %}
    {% include "@molecules/page-heading/page-heading.twig" %}
  {% endif %}
  <div class="wrapper">
    {% if contact_gmap %}
      {{ contact_gmap }}
    {% else %}
      {% include "@atoms/iframe/iframe.twig" with {
        "class": "gmap-contact"
      } %}
    {% endif %}
    <div class="message-section">
      {% if contact_form %}
        {{ contact_form }}
      {% else %}
        <div class="map-with-form">
          {% include "@molecules/message/message.twig" %}
        </div>
      {% endif %}
      <div class="contact-info-wrapper">
        {% if contact_info %}
          {{ contact_info }}
        {% else %}
          {% include "@molecules/info/info.twig" %}
        {% endif %}
      </div>
    </div>
  </div>
</div>
<div class="subscribe">
  {% if drupal_subscribe %}
    {{ drupal_subscribe }}
  {% else %}
    {% include "@molecules/subscribe/subscribe.twig" %}
  {% endif %}
</div>

